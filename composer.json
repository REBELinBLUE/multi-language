{
    "name": "michele-angioni/multi-language",
    "description": "A Laravel package to handle multi language localization.",
    "keywords": ["laravel", "localization", "language"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michele Angioni",
            "email": "michele.angioni@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "^5.5 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^5.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "MicheleAngioni\\MultiLanguage\\": "src/MicheleAngioni/MultiLanguage"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
